{
  "name": "vscode-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm install && cp pre-commit .git/hooks/pre-commit",
    "start": "npm install && next dev --hostname=0.0.0.0",
    "build": "NEXTEXPORT=1 next build && rm -r docs",
    "postbuild": "mv out docs && cp CNAME docs/CNAME && touch docs/.nojekyll && git add . && echo Build succeeded and changes added, review before pushing!"
  },
  "dependencies": {
    "next": "^15.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
